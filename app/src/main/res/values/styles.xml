<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="custom_bottom_sheet_btn_colors">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">@dimen/_24sdp</item>
        <item name="android:layout_margin">@dimen/_4sdp</item>
    </style>

    <style name="match_wrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="match_match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="wrap_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="history_one_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">@dimen/_8sdp</item>
        <item name="android:paddingStart">@dimen/_8sdp</item>
        <item name="android:paddingEnd">@dimen/_8sdp</item>
        <item name="android:layout_marginEnd">@dimen/_8sdp</item>
    </style>

    <style name="w_24_h_24">
        <item name="android:layout_width">@dimen/_24sdp</item>
        <item name="android:layout_height">@dimen/_24sdp</item>
    </style>

    <style name="w_16_h_16">
        <item name="android:layout_width">@dimen/_16sdp</item>
        <item name="android:layout_height">@dimen/_16sdp</item>
    </style>

    <style name="w_32_h_32">
        <item name="android:layout_width">@dimen/_32sdp</item>
        <item name="android:layout_height">@dimen/_32sdp</item>
    </style>

    <style name="container_all_cards">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_margin">@dimen/_16sdp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="app_b">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?actionBarSize</item>
        <item name="android:paddingStart">@dimen/_16sdp</item>
        <item name="android:paddingTop">@dimen/_8sdp</item>
        <item name="android:paddingEnd">@dimen/_16sdp</item>
        <item name="android:paddingBottom">@dimen/_8sdp</item>
    </style>

    <style name="app_b_description">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="app_b_liner">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_1sdp</item>
        <item name="android:background">@color/grey_700</item>
    </style>

    <style name="met_phone">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/background_custom_edittext</item>
        <item name="android:inputType">phone</item>
        <item name="android:lines">1</item>
        <item name="android:maxLength">17</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/teal_500</item>
    </style>

    <style name="tv_error">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/_2sdp</item>
        <item name="android:textColor">@color/red_400</item>
        <item name="android:textSize">8sp</item>
    </style>

    <style name="btn_t" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/_8sdp</item>
        <item name="android:background">@color/white</item>
        <item name="android:textColor">@color/teal_500</item>
        <item name="android:textSize">8sp</item>
    </style>

    <style name="btn_bottom">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@drawable/click_effect_custom_button</item>
        <item name="android:padding">@dimen/_14sdp</item>
    </style>

    <style name="ed_t_user_name">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/_8sdp</item>
        <item name="android:layout_marginEnd">@dimen/_16sdp</item>
        <item name="android:background">@drawable/background_custom_edittext</item>
        <item name="android:inputType">text</item>
        <item name="android:lines">1</item>
        <item name="android:maxLength">20</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/teal_500</item>
    </style>

    <style name="ed_t_password">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/_8sdp</item>
        <item name="android:background">@drawable/background_custom_edittext</item>
        <item name="android:inputType">text</item>
        <item name="android:lines">1</item>
        <item name="android:maxLength">20</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/teal_500</item>
    </style>

    <style name="met_verify_code">
        <item name="android:layout_width">@dimen/_120sdp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">@dimen/_8sdp</item>
        <item name="android:background">@drawable/background_custom_edittext</item>
        <item name="android:lines">1</item>
        <item name="android:maxLength">8</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/teal_500</item>
    </style>

    <style name="app_b_btn_close">
        <item name="android:layout_width">@dimen/_24sdp</item>
        <item name="android:layout_height">@dimen/_24sdp</item>
        <item name="android:layout_gravity">center_vertical|end</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="met_pan_card">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/_8sdp</item>
        <item name="android:layout_marginEnd">@dimen/_16sdp</item>
        <item name="android:background">@drawable/background_custom_edittext</item>
        <item name="android:inputType">phone</item>
        <item name="android:lines">1</item>
        <item name="android:maxLength">19</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/teal_500</item>
    </style>
</resources>